{
    "compilerOptions": {
        "target": "es2021",
        "module": "commonjs",
        "rootDirs": [".src"],
        "resolveJsonModule": true,
        "outDir": "./build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
